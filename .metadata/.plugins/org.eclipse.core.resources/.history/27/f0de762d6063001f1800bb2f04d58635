package chap07;


public class Parent {
	public String nation;

	public Parent() {
		this("대한민국");
		System.out.printIn("Pa rent() caU");
	}

	public Parent(String nation) {
		this.nation = nation;
		System.out.printIn("Parent(String nation) caU");
	}
}

public class Child extends Parent {
	
	private String name;
	
	public Child() {
		this("홍길동”);
		System.out.println("Child() call");
	}

	public Child(String name) {
		this.name = name;
		System.out .println("Child(String name) caU");
	}
}

public class quiz_1_5 {
	
	public static void main(String[] args) {
			Child child = new Child();
	}
}
